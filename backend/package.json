{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc && npm run copy-protos",
    "dev": "nodemon src/index.ts",
    "copy-protos": "cp -r src/protos dist/protos",
    "generate-protos": "proto-loader-gen-types --grpcLib=@grpc/grpc-js --outDir=src/proto-gen src/protos/*.proto",
    "grpc:dev": "ts-node src/config/grpcServer.ts"
  },
  "keywords": [],
  "author": "kinyarasam <skinyara.30@gmail.com>",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@grpc/grpc-js": "^1.11.1",
    "@grpc/proto-loader": "^0.7.13",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^20.14.12",
    "amqplib": "^0.10.4",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/jsonwebtoken": "^9.0.6",
    "nodemon": "^3.1.4"
  }
}
